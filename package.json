{
  "name": "@vigour-io/play-player",
  "description": "Play-player component for use with Brisky",
  "version": "1.1.3",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/play-player.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vigour-io/play-player/issues"
  },
  "scripts": {
    "start": "budo ./test/index.js:bundle.js -d ./test --css test/bundle.css --pushstate --live -- -r ./package.json:package.json",
    "browserstack": "browserify node_modules/browserstack-tape-reporter test -o test/bundle.js -r ./package.json:package.json && cd test && browserstack-runner | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0) && standard"
  },
  "dependencies": {
    "@vigour-io/css": "^1.0.0",
    "@vigour-io/play-icon": "^1.0.0",
    "@vigour-io/play-lock": "^1.1.0",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "brisky-core": "^1.3.3",
    "brisky-events": "^1.2.0",
    "envify": "^3.4.0",
    "postcssify": "^2.1.1",
    "prefix-style": "^2.0.1",
    "tiny-load-script": "^1.0.0",
    "vigour-stamp": "^1.2.0",
    "vigour-ua": "^2.0.0"
  },
  "devDependencies": {
    "@vigour-io/play-state-player": "^1.0.0",
    "brisky": "^1.3.1",
    "browserstack-runner": "^0.4.4",
    "browserstack-tape-reporter": "git+https://github.com/vigour-io/browserstack-tape-reporter.git",
    "budo": "^8.2.2",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "pre-commit": "^1.1.3",
    "standard": "^7.0.1",
    "tap-difflet": "0.4.0",
    "tape": "^4.6.0",
    "vigour-state": "^3.5.0"
  },
  "browser": {
    "./test/fn": "./test/fn/browser"
  },
  "homepage": "https://github.com/vigour-io/player",
  "keywords": [
    "brisky",
    "player",
    "DOM"
  ],
  "contributors": [
    "Youri Daamen <youri@vigour.io>"
  ],
  "engines": {
    "node": ">=4"
  },
  "browserify": {
    "transform": [
      "envify",
      "postcssify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
